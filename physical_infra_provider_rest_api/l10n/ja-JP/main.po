"   \"credentials\": {\n"
"      \"password\":\"&lt;new_passowrd&gt;\",\n"
"      \"userid\":\"&lt;new_user_name&gt;\"\n"
"   },\n"
"   \"hostname\":\"&lt;New_Lenovo_XClarity_Administrator_address&gt;\",\n"
"   \"name\":\"&lt;new_provider_name&gt;\",\n"
